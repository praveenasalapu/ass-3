#include <stdio.h>
int main() {
   int num , prime = 0;
   scanf("%d",&num);
   for(int i=2 ; i < num/2 ; i++) {
      if(num%i == 0) {
         printf("%d is not a prime number", num);
         prime = 1;
         break;
      }
   }
   if(prime == 0) {
      printf("%d is a prime number", num);
   }
}
